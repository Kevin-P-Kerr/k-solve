// the axioms
a = forall a : ~MAN(a) + MORTAL(a) 
b = forall b: ~SOC(b) + MAN(b)
c = forall c thereis d: ~MAN(c) + MOTHER(d c)
d = forall e forall f: ~MOTHER(e f) + MAN(e)

e = a*b
e = e*c
e = e*d


e = replace(e,a,b)
e = replace(e,a,c)
e = replace(e,d,e)
e = replace(e,a,f)
e = e * forall a: SOC(a)
e = simplify(e)
print(e)
